{
  "name": "checkmate",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"yarn workspace web dev\" \"yarn workspace server dev\"",
    "dev:web": "turbo run dev --filter=web",
    "dev:mobile": "turbo run dev --filter=mobile",
    "build": "turbo run build",
    "clean": "rm -rf node_modules yarn.lock && yarn install",
    "seed": "node --loader ts-node/esm ./server/scripts/seed.ts"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/yamljs": "^0.2.34",
    "concurrently": "^9.2.0",
    "knex": "2.5.1",
    "ts-node": "^10.9.2",
    "turbo": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@fontsource/jetbrains-mono": "^5.2.6",
    "add": "^2.0.6",
    "dotenv": "^17.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0",
    "yarn": "^1.22.22",
    "zustand": "^5.0.6"
  },
  "resolutions": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  }
}
